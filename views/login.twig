{% extends "layout.twig" %}

{% block content %}
    <h1>Login</h1>
    <form action="../RepasoPHP/login" method="post">
        <label for="email">Email: </label>
        <input type="email" name="email">
        <br>
        <label for="password">Password: </label>
        <input type="password" name="password">
        <br>
        <button type="submit">Login</button>
    </form>
{% endblock %}